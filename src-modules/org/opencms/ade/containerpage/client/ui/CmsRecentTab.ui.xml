<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cnt="urn:import:org.opencms.ade.containerpage.client.draganddrop">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="containerpage" type="org.opencms.ade.containerpage.client.ui.css.I_CmsLayoutBundle"></ui:with>
	<ui:style>
		.itemList{
			max-height: 412px;
		}
		.description{
			margin-bottom: 4px;
		}
	</ui:style>
	<g:FlowPanel styleName="{containerpage.containerpageCss.clipboardTabPanel}">
		<g:Label ui:field="m_decriptionLabel" addStyleNames="{style.description} {resources.generalCss.textBig}"></g:Label>
		<cnt:CmsDragTargetList ui:field="m_listPanel" addStyleNames="{style.itemList} {containerpage.containerpageCss.clipboardList} {resources.generalCss.cornerAll}" ></cnt:CmsDragTargetList>
	</g:FlowPanel>
</ui:UiBinder> 