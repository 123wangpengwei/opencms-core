<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- should replaced CmsTabInnerPanel.ui.xml -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="galleryResources" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	
	<g:HTMLPanel ui:field="m_tab" styleName="{galleryResources.galleryDialogCss.tabPanel}">
		<cms:CmsFlowPanel ui:field="m_params" tag="div">
		    <cms:CmsFlowPanel ui:field="m_types" tag="span" addStyleNames="{resources.generalCss.cornerAll}"></cms:CmsFlowPanel>	
			<cms:CmsFlowPanel ui:field="m_galleries" tag="span" addStyleNames="{resources.generalCss.cornerAll}"></cms:CmsFlowPanel>
			<cms:CmsFlowPanel ui:field="m_categories" tag="span" addStyleNames="{resources.generalCss.cornerAll}"></cms:CmsFlowPanel>			
		</cms:CmsFlowPanel>
		<cms:CmsFlowPanel ui:field="m_options" tag="div" addStyleNames="{galleryResources.galleryDialogCss.marginBottom}">
			<!-- <cms:CmsFlowPanel ui:field="m_sortLabel" tag="span"></cms:CmsFlowPanel>	
			<cms:CmsFlowPanel ui:field="m_sortSelectBox" tag="span"></cms:CmsFlowPanel>
			<cms:CmsFlowPanel ui:field="m_localeLabel" tag="span"></cms:CmsFlowPanel>	
			<cms:CmsFlowPanel ui:field="m_localeSelectBox" tag="span"></cms:CmsFlowPanel>-->
		</cms:CmsFlowPanel>
		<cms:CmsFlowPanel ui:field="m_list" tag="div" styleName="{galleryResources.galleryDialogCss.criteriaList}"  addStyleNames="{resources.generalCss.cornerAll}">
			<cms:CmsList ui:field="m_scrollList" >
			</cms:CmsList>
		</cms:CmsFlowPanel>			
	</g:HTMLPanel> 
	
</ui:UiBinder> 