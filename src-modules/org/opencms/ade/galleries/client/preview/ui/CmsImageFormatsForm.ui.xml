<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui"
	xmlns:cmsinput="urn:import:org.opencms.gwt.client.ui.input">
	<ui:with field="galleryResources" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel ui:field="m_panel">			
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLine}">		 
			    <cmsinput:CmsLabel ui:field="m_selectBoxLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}" />	 									
			    <g:SimplePanel styleName="{galleryResources.previewDialogCss.formatSelectBox}">
			    	<cmsinput:CmsSelectBox ui:field="m_selectBox" />
			    </g:SimplePanel>				 									
				<cms:CmsPushButton ui:field="m_cropButton" addStyleNames="{galleryResources.previewDialogCss.formatButton}" />				 				
				<cms:CmsPushButton ui:field="m_removeCropButton" addStyleNames="{galleryResources.previewDialogCss.formatButton}" /> 
			</g:FlowPanel>
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLineSize}">
				<cmsinput:CmsLabel ui:field="m_widthLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}" />
				<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatText}">
					<cmsinput:CmsTextBox ui:field="m_widthBox" />
				</g:FlowPanel>
			</g:FlowPanel>
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLineSize}">
				<cmsinput:CmsLabel ui:field="m_heightLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}" />
				<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatText}">
					<cmsinput:CmsTextBox ui:field="m_heightBox" />
				</g:FlowPanel>
			</g:FlowPanel>	
		</g:FlowPanel>
</ui:UiBinder> 