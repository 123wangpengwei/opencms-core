<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 	
	xmlns:cms="urn:import:org.opencms.gwt.client.ui"
	xmlns:cmsinput="urn:import:org.opencms.gwt.client.ui.input">
	<ui:with field="galleryResources" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel>
		<g:FlowPanel ui:field="m_panel" styleName="{galleryResources.previewDialogCss.formatsPanel}" >			
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLine}">		 
			    <cmsinput:CmsLabel ui:field="m_selectBoxLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}"></cmsinput:CmsLabel>	 									
			    <cms:CmsFlowPanel tag='span' addStyleNames="{galleryResources.previewDialogCss.formatSelectBox}">
			    	<cmsinput:CmsSelectBox ui:field="m_selectBox" ></cmsinput:CmsSelectBox>
			    </cms:CmsFlowPanel>				 									
				<cms:CmsPushButton ui:field="m_cropButton" addStyleNames="{galleryResources.previewDialogCss.formatButton}"></cms:CmsPushButton>				 				
				<cms:CmsPushButton ui:field="m_removeCropButton" addStyleNames="{galleryResources.previewDialogCss.formatButton}"></cms:CmsPushButton> 
			</g:FlowPanel>
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLineSize}">
				<cmsinput:CmsLabel ui:field="m_widthLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}"></cmsinput:CmsLabel>
				<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatText}">
					<cmsinput:CmsTextBox ></cmsinput:CmsTextBox>
				</g:FlowPanel>
			</g:FlowPanel>
			<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatsLineSize}">
				<cmsinput:CmsLabel ui:field="m_heightLabel" addStyleNames="{galleryResources.previewDialogCss.formatLabel}"></cmsinput:CmsLabel>
				<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.formatText}">
					<cmsinput:CmsTextBox ></cmsinput:CmsTextBox>
				</g:FlowPanel>
			</g:FlowPanel>	
		</g:FlowPanel>
		<g:FlowPanel addStyleNames="{galleryResources.previewDialogCss.previewButtonBar}">
			<cms:CmsPushButton ui:field="m_selectButton" addStyleNames="{galleryResources.previewDialogCss.previewButton}"></cms:CmsPushButton>			
		</g:FlowPanel>
	</g:FlowPanel>	
</ui:UiBinder> 