<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {

		}
		
		.decorationBox {
			float:left;
			clear: both;
			margin-top: 5px;
		}
		
		@if user.agent ie6 {
			.decorationBox * { 
				display: inline;
			}
		} 
		
		.primary {
			
		}
		
	
	</ui:style>
	<g:FlowPanel ui:field="m_panel" styleName="{style.panel}">
		<g:FlowPanel ui:field="m_decorationBox"  styleName="{style.decorationBox}"></g:FlowPanel>
		<g:FlowPanel ui:field="m_primary" styleName="{style.primary}"></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>