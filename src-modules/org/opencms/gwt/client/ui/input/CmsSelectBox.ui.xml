<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cms="urn:import:org.opencms.gwt.client.ui.input">
	<ui:with field="layout"
		type="org.opencms.gwt.client.ui.css.I_CmsInputLayoutBundle" />
	<ui:style>
	</ui:style>
	<g:FlowPanel ui:field="m_panel" styleName="{layout.inputCss.selectBoxRoot}">
		<g:FocusPanel ui:field="m_opener"
			styleName="{layout.inputCss.selectBoxSelected}">
			<g:FlowPanel>
				<g:HTML ui:field="m_arrow">
				</g:HTML>
				<g:HTML ui:field="m_openerHtml" styleName = "{layout.inputCss.selectBoxOpener}">
				</g:HTML>
			</g:FlowPanel>
		</g:FocusPanel>
		<cms:CmsErrorWidget ui:field="m_error" />
	</g:FlowPanel>
</ui:UiBinder>