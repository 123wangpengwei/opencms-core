<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTMLPanel tag="li" styleName="{resources.listItemCss.listItem}">
		<div class="{resources.listItemCss.itemContainer}">
			<div class="{resources.listItemCss.itemContent}">
				<div ui:field="m_iconDiv"></div>
		    	<g:FlowPanel ui:field="m_titleRow"><g:Label ui:field="m_titleDiv" styleName="{resources.listItemCss.itemTitle}"></g:Label></g:FlowPanel>
				<g:Label ui:field="m_subTitleDiv"></g:Label>
			</div>
			<div class="{resources.listItemCss.itemAdditional}" ui:field="m_additionalDiv"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 