<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTML styleName="{resources.highlightCss.highlightBox}">
		<div class="{resources.highlightCss.borderTop}"></div>
		<div class="{resources.highlightCss.borderRight}"></div>
		<div class="{resources.highlightCss.borderBottom}"></div>
		<div class="{resources.highlightCss.borderLeft}"></div>	
	</g:HTML>
</ui:UiBinder>