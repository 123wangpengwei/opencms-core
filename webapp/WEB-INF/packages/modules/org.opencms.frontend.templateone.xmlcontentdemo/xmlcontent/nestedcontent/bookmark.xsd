<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	
	<xsd:include schemaLocation="opencms://opencms-xmlcontent.xsd"/>
	<xsd:include schemaLocation="opencms://sites/default/xmlcontent/nestedcontent/address.xsd"/>
	
	<xsd:element name="Bookmarks" type="OpenCmsBookmarks"/>
	
	<xsd:complexType name="OpenCmsBookmarks">
		<xsd:sequence>
			<xsd:element name="Bookmark" type="OpenCmsBookmark" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OpenCmsBookmark">
		<xsd:sequence>
			<xsd:element name="Title" type="OpenCmsString" />
			<xsd:element name="Description" type="OpenCmsHtml" minOccurs="0" maxOccurs="1" />
			<xsd:element name="Logo" type="OpenCmsVfsFile" minOccurs="0" />
			<xsd:element name="Contact" type="OpenCmsAddress" minOccurs="0" maxOccurs="3" />
		</xsd:sequence>
		<xsd:attribute name="language" type="OpenCmsLocale" use="optional"/>
	</xsd:complexType>

	<xsd:annotation>
		<xsd:appinfo>
			<mappings>
				<mapping element="Title" mapto="property:Title" />
			</mappings>
			<layouts>
				<layout element="Logo" widget="ImageGalleryWidget" />
			</layouts>				
		</xsd:appinfo>
	</xsd:annotation>  
</xsd:schema>