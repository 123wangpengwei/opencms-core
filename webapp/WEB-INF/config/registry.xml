<?xml version="1.0" encoding="ISO-8859-1"?>
<registry>
	<system>
		<importexport>
            <handler>com.opencms.legacy.CmsCosImportExportHandler</handler>
            <handler>org.opencms.importexport.CmsVfsImportExportHandler</handler>
            <handler>org.opencms.importexport.CmsModuleImportExportHandler</handler>
			<vfsdataimport>
				<class>com.opencms.legacy.CmsImportVersion1</class>
				<class>org.opencms.importexport.CmsImportVersion2</class>
				<class>org.opencms.importexport.CmsImportVersion3</class>
				<class>org.opencms.importexport.CmsImportVersion4</class>
			</vfsdataimport>
		</importexport>
		<dialoghandler>
			<class>org.opencms.workplace.CmsDelete</class>
			<class>org.opencms.workplace.CmsLock</class>
			<class>org.opencms.workplace.CmsProperty</class>
		</dialoghandler>
		<editorhandler>
			<class>org.opencms.workplace.editor.CmsEditorHandler</class>
		</editorhandler>
		<editoraction>
			<class>org.opencms.workplace.editor.CmsEditorActionDefault</class>
		</editoraction>
		<localehandler>
			<class>org.opencms.i18n.CmsDefaultLocaleHandler</class>
		</localehandler>
		<resourceinit />
		<synchronizemodifications />
		<view>
			<name>select.explorer</name>
			<url>/system/workplace/jsp/explorer_fs.html</url>
		</view>
		<view>
			<name>select.admin</name>
			<url>/system/workplace/action/administration.html</url>
		</view>
		<view>
			<name>select.tasks</name>
			<url>/system/workplace/action/tasks.html</url>
		</view>
		<exportpoint>
			<source>/system/workplace/resources/</source>
			<destination>resources/</destination>
		</exportpoint>
		<exportpoint>
			<source>/system/workplace/skins/</source>
			<destination>skins/</destination>
		</exportpoint>
		<smtpserver>my.smtp.server</smtpserver>
		<smtpserver2>alternative.smtp.server</smtpserver2>
		<defaultmailsender>nobody@nowhere.com</defaultmailsender>
		<checklink>
			<mailfrom>nobody@nowhere.com</mailfrom>
			<mailto>nobody@nowhere.com</mailto>
			<mailtemplate>/system/workplace/templates/urlcheckform</mailtemplate>
		</checklink>
		<tempfileproject></tempfileproject>
		<UseWpPicturesFromVFS>false</UseWpPicturesFromVFS>
		<searchbylucene>off</searchbylucene>
		<history>
			<enabled>true</enabled>
			<weeks></weeks>
			<deleteversions>false</deleteversions>
			<versions>10</versions>
		</history>
		<search>
            <directory>index</directory>
            <mergefactor>100000</mergefactor>
            <timeout>60000</timeout>
            <cosindexer>com.opencms.legacy.CmsCosIndexer</cosindexer>
            <vfsindexer>org.opencms.search.CmsVfsIndexer</vfsindexer>
            <documenttype>
                <name>xmlpage</name>
				<class>org.opencms.search.documents.CmsXmlPageDocument</class>
				<resourcetype>org.opencms.file.CmsResourceTypeXmlPage</resourcetype>
				<mimetype>text/html</mimetype>
            </documenttype>
            <documenttype>
                <name>page</name>
                <class>com.opencms.legacy.CmsPageDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypePage</resourcetype>
                <mimetype>text/html</mimetype>
            </documenttype>
            <documenttype>
			    <name>text</name>
			    <class>org.opencms.search.documents.CmsTextDocument</class>
			    <resourcetype>org.opencms.file.CmsResourceTypePlain</resourcetype>
                <mimetype>text/html</mimetype>
            </documenttype>
            <documenttype>
                <name>jsp</name>
                <class>org.opencms.search.documents.CmsTextDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypeJsp</resourcetype>
            </documenttype>
            <documenttype>
                <name>pdf</name>
                <class>org.opencms.search.documents.CmsPdfDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypeBinary</resourcetype>
                <resourcetype>org.opencms.file.CmsResourceTypePlain</resourcetype>
                <mimetype>application/pdf</mimetype>
            </documenttype>
            <documenttype>
                <name>msword</name>
                <class>org.opencms.search.documents.CmsWordDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypeBinary</resourcetype>
                <resourcetype>org.opencms.file.CmsResourceTypePlain</resourcetype>
                <mimetype>application/msword</mimetype>
            </documenttype>
            <documenttype>
                <name>msexcel</name>
                <class>org.opencms.search.documents.CmsExcelDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypeBinary</resourcetype>
                <resourcetype>org.opencms.file.CmsResourceTypePlain</resourcetype>
                <mimetype>application/vnd.ms-excel</mimetype>
            </documenttype>
            <documenttype>
                <name>image</name>
                <class>org.opencms.search.documents.CmsVfsDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypeImage</resourcetype>
            </documenttype>
            <documenttype>
                <name>generic</name>
                <class>org.opencms.search.documents.CmsVfsDocument</class>
                <resourcetype>org.opencms.file.CmsResourceTypePlain</resourcetype>
            </documenttype>
            <analyzer>
                <class>org.apache.lucene.analysis.de.GermanAnalyzer</class>
                <lang>de</lang>
            </analyzer>
            <analyzer>
                <class>org.apache.lucene.analysis.standard.StandardAnalyzer</class>
                <lang>en</lang>
            </analyzer>
            <analyzer>
                <class>org.apache.lucene.analysis.snowball.SnowballAnalyzer</class>
                <param>French</param>
                <lang>fr</lang>
            </analyzer>
            <analyzer>
                <class>org.apache.lucene.analysis.snowball.SnowballAnalyzer</class>
                <param>Italian</param>
                <lang>it</lang>
            </analyzer>
            <index>
                <name>Online project</name>
                <rebuild>auto</rebuild>
                <project>Online</project>
                <site>/sites/default/</site>
                <lang>en</lang>
                <folder>
                	<source>/</source>
                	<documenttype>xmlpage</documenttype>
                	<documenttype>page</documenttype>
                	<documenttype>text</documenttype>
                	<documenttype>pdf</documenttype>
                	<documenttype>msword</documenttype>
                	<documenttype>msexcel</documenttype>
                	<documenttype>image</documenttype>
                	<documenttype>generic</documenttype>
                 </folder>
            </index>
            <index>
                <name>Offline project</name>
                <rebuild>auto</rebuild>
                <project>Offline</project>
                <site>/sites/default/</site>
                <lang>en</lang>
                <folder>
                	<source>/</source>
                	<documenttype>xmlpage</documenttype>
                	<documenttype>page</documenttype>
                	<documenttype>text</documenttype>
                	<documenttype>pdf</documenttype>
                	<documenttype>msword</documenttype>
                	<documenttype>msexcel</documenttype>
                	<documenttype>image</documenttype>
                	<documenttype>generic</documenttype>
                 </folder>
            </index>
            <index>
                <name>System folder</name>
                <rebuild>auto</rebuild>
                <project>Online</project>
                <site>/</site>
                <lang>en</lang>
                <folder>
                	<source>/system/</source>
                	<documenttype>xmlpage</documenttype>
                	<documenttype>page</documenttype>
                	<documenttype>text</documenttype>
                	<documenttype>jsp</documenttype>
                	<documenttype>generic</documenttype>
               	</folder>                
            </index>
        </search>
		<crontab>
			<!--
		    <cronjob>
		    	<min>*</min>
		        <hour>*</hour>
		        <dayofmonth>*</dayofmonth>
		        <month>*</month>
		        <dayofweek>*</dayofweek>
				<class>org.opencms.monitor.CmsMemoryMonitor</class>
		        <user>cmsadmin</user>
				<group>cmsadministrators</group>
				<params></params>
		    </cronjob>
			//-->
		</crontab>
		@REGISTRY_CONFIGURATION@
	</system>
	<modules/>
</registry>
