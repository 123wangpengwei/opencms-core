<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE opencms SYSTEM "http://www.opencms.org/dtd/6.0/opencms-vfs.dtd">

<opencms>    
    <vfs>
    	<resources>
			<resourceloaders>
				<loader class="com.opencms.legacy.CmsXmlTemplateLoader">            
					<param name="elementcache.enabled">true</param>
					<param name="elementcache.uri">10000</param>
					<param name="elementcache.elements">50000</param>
					<param name="elementcache.variants">100</param>
				</loader>
				<loader class="org.opencms.loader.CmsDumpLoader" />
				<loader class="org.opencms.loader.CmsPointerLoader" />
				<loader class="org.opencms.loader.CmsJspLoader">
					<param name="jsp.repository">C:\TEMP\</param>
					<param name="jsp.folder">/WEB-INF/jsp/</param>
					<param name="jsp.errorpage.committed">true</param>
				</loader>
				<loader class="org.opencms.loader.CmsXmlPageLoader" />
			</resourceloaders>    
			<resourcetypes>
				<type class="com.opencms.legacy.CmsResourceTypeXMLTemplate" />
				<type class="org.opencms.file.types.CmsResourceTypeFolder" />
				<type class="org.opencms.file.types.CmsResourceTypePlain">
					<mapping suffix=".txt" />
					<mapping suffix=".html" />
					<mapping suffix=".htm" />
				</type>
				<type class="org.opencms.file.types.CmsResourceTypePointer" />
				<type class="org.opencms.file.types.CmsResourceTypeBinary">
					<mapping suffix=".pdf" />
					<mapping suffix=".zip" />
					<mapping suffix=".class" />
					<mapping suffix=".ppt" />
					<mapping suffix=".doc" />
					<mapping suffix=".xls" />
				</type>
				<type class="org.opencms.file.types.CmsResourceTypeImage">
					<mapping suffix=".jpeg" />
					<mapping suffix=".jpg" />
					<mapping suffix=".gif" />
					<mapping suffix=".png" />
					<mapping suffix=".tif" />
					<mapping suffix=".tiff" />
				</type>
				<type class="org.opencms.file.types.CmsResourceTypeJsp">
					<mapping suffix=".jsp" />
				    <param name="default.encoding">UTF-8</param>
				</type>
				<type class="org.opencms.file.types.CmsResourceTypeXmlPage" />
				<type class="org.opencms.file.types.CmsResourceTypeXmlContent">
					<param name="resource.type.id">11</param>
					<param name="resource.type.name">xmlcontent</param>
				</type>	
			</resourcetypes>  
			<collectors>
				<collector class="org.opencms.file.CmsDefaultResourceCollector" order="100" />
			</collectors>			
        </resources>   
		<defaultfiles>
			<defaultfile name="index.html" />
			<defaultfile name="index.jsp" />
		</defaultfiles>        
        <translations>
        	<filetranslations enabled="true">
        		<translation>s#[\s]+#_#g</translation>
        		<translation>s#\\#/#g</translation>
        		<translation>s#[^0-9a-zA-Z_\.\-\/]#!#g</translation>
        		<translation>s#!+#x#g</translation>
        	</filetranslations>
        	<foldertranslations enabled="false">
        		<translation>s#/default/vfs/content/bodys/(.*)#/system/bodies/$1#</translation>
        		<translation>s#/default/vfs/pics/system/(.*)#/system/workplace/resources/$1#</translation>
        		<translation>s#/default/vfs/pics/(.*)#/system/galleries/pics/$1#</translation>
        		<translation>s#/default/vfs/download/(.*)#/system/galleries/download/$1#</translation>
        		<translation>s#/default/vfs/externallinks/(.*)#/system/galleries/externallinks/$1#</translation>
        		<translation>s#/default/vfs/htmlgalleries/(.*)#/system/galleries/htmlgalleries/$1#</translation>
        		<translation>s#/default/vfs/content/(.*)#/system/modules/org.opencms.default/$1#</translation>
        		<translation>s#/default/vfs/moduledemos/(.*)#/system/moduledemos/$1#</translation>
        		<translation>s#/default/vfs/system/workplace/config/language/(.*)#/system/workplace/locales/$1#</translation>
        		<translation>s#/default/vfs/system/workplace/css/(.*)#/system/workplace/resources/$1#</translation>
        		<translation>s#/default/vfs/system/workplace/templates/js/(.*)#/system/workplace/scripts/$1#</translation>
        	</foldertranslations>
        </translations>     
		<xmlcontents>	
			<xmlcontent class="org.opencms.xml.types.CmsXmlBooleanValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlBooleanWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlColorValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlColorpickerWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlDateTimeValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlDateTimeWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlHtmlValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlHtmlWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlLocaleValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlStringWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlSimpleHtmlValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlHtmlWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlStringValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlStringWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlVfsFileValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlVfsFileWidget" />
			<xmlcontent class="org.opencms.xml.types.CmsXmlVfsImageValue" widget="org.opencms.workplace.xmlwidgets.CmsXmlVfsImageWidget" />
		</xmlcontents>              
    </vfs>   
</opencms>    