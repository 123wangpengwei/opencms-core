    <script language="JavaScript">

        function getMenue(wert){
            res=]]><PROCESS>resint</PROCESS><![CDATA[;
            document.write("<table border=5 rules=groups bgcolor=#C0C0C0 width=100%>");
            for(i=0; i<top.vi.menus[res].items.length; i++){
                if(top.vi.menus[res].items[i].rules.charAt(wert)=='a'){
                    if(top.vi.menus[res].items[i].name=='-'){
                        document.write("<tr><td><hr></td></tr>");
                    }else{
                        document.write("<tr><td class=kl>"+ top.vi.menus[res].items[i].name +"</td></tr>");
                    }
                }else{
                    if(top.vi.menus[res].items[i].rules.charAt(wert)=='i'){
                        if(top.vi.menus[res].items[i].name=='-'){
                            document.write("<tr><td><hr></td></tr>");
                        }else{
                            document.write("<tr><td class=inactive>"+ top.vi.menus[res].items[i].name +"</td></tr>");
                        }
                    }
                }
            }
            document.write("</table>");

        }
    </script>
