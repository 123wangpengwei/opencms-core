<xmltemplate>

	<template>
		<METHOD name="getSingleNews"/>
	</template>

	<newsentry>
		<![CDATA[
		<fo:block font-size="18pt" 
			 line-height="26pt"
			 space-after.optimum="15pt"
			 background-color="#A0A0A0"
			 color="white"
			 text-align="centered"
			 padding-top="4pt">]]>
			 Headline: <PROCESS>headline</PROCESS><![CDATA[
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="5pt" text-align="justified">]]>
			Id: <PROCESS>id</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" color="#000000" text-align="justified" space-before.optimum="25pt">]]>
			 Description: <PROCESS>description</PROCESS><![CDATA[
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			Text: <PROCESS>text</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			Author: <PROCESS>author</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			Link: <PROCESS>link</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			LinkText: <PROCESS>linkText</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			Date: <PROCESS>date</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			Channel: <PROCESS>channel</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			A_info1: <PROCESS>a_info1</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			A_info2: <PROCESS>a_info2</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:block font-size="12pt" space-before.optimum="25pt" text-align="justified">]]>
			A_info3: <PROCESS>a_info3</PROCESS><![CDATA[	
		</fo:block>
		
		<fo:table>
 			<fo:table-column column-width="10cm" /> 
 			<fo:table-column column-width="6cm" /> 
			<fo:table-body>
				<fo:table-row>
					<fo:table-cell>
						<fo:block font-size="9pt" color="#000000" space-before.optimum="20pt" text-align="start">
						<fo:simple-link external-destination="]]><PROCESS>link</PROCESS><![CDATA[">]]>
						<PROCESS>linkText</PROCESS><![CDATA[
						</fo:simple-link>
						</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-size="9pt" color="#000000" space-before.optimum="20pt" text-align="end">]]>
							<PROCESS>author</PROCESS><![CDATA[, ]]><PROCESS>date</PROCESS><![CDATA[
						</fo:block> 
					</fo:table-cell>
				</fo:table-row>
			</fo:table-body>
		</fo:table>]]>
	</newsentry>	
	
	
	<paragraphsep><![CDATA[
	<fo:block font-size="12pt" line-height="16pt" space-before.optimum="10pt"></fo:block>
	]]></paragraphsep>
	
</xmltemplate>