function checkFormData()
{
    var result=true;
    var at_pos, entry, strLength;

    entry = document.DBEXPORT.VNAME.value;
    strLength=entry.length;
    if (strLength < 1){
        alert(']]><LABEL value="error.reason.namemissing" /><![CDATA[');
