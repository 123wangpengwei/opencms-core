<?xml version="1.0" encoding="ISO-8859-1"?>
<registry>
    <system>
        <resourcetype>
            <class>com.opencms.file.CmsResourceTypeFolder</class>
            <class>com.opencms.file.CmsResourceTypePage</class>
            <class>com.opencms.file.CmsResourceTypePointer</class>
            <class>com.opencms.file.CmsResourceTypePlain</class>
            <class>com.opencms.file.CmsResourceTypeXMLTemplate</class>
            <class>com.opencms.file.CmsResourceTypeBinary</class>
            <class>com.opencms.file.CmsResourceTypeImage</class>
            <class>com.opencms.file.CmsResourceTypeCompatiblePlain</class>
            <class>com.opencms.file.CmsResourceTypeJsp</class>
            <class>com.opencms.file.CmsResourceTypeNewPage</class>
        </resourcetype>  
        <resourceloader>
            <class>org.opencms.loader.CmsXmlTemplateLoader</class>
            <class>org.opencms.loader.CmsDumpLoader</class>
            <class>org.opencms.loader.CmsPointerLoader</class>
            <class>org.opencms.loader.CmsJspLoader</class>
            <class>org.opencms.loader.CmsPageLoader</class>
        </resourceloader>        
        <view>
            <name>select.explorer</name>
            <url>/system/workplace/action/explorer_new.html</url>
        </view>
        <view>
            <name>select.admin</name>
            <url>/system/workplace/action/administration.html</url>
        </view>
        <view>
            <name>select.tasks</name>
            <url>/system/workplace/action/tasks.html</url>
        </view>
        <view>
            <name>select.explorer</name>
            <url>/system/workplace/jsp/explorer_fs.html</url>
        </view>        
        <exportpoint>
            <source>/system/workplace/resources/</source>
            <destination>resources/</destination>
        </exportpoint>
        <exportpoint>
            <source>/system/workplace/skins/</source>
            <destination>skins/</destination>
        </exportpoint>     
        <smtpserver>my.smtp.server</smtpserver>
        <smtpserver2>alternative.smtp.server</smtpserver2>
        <defaultmailsender>nobody@nowhere.com</defaultmailsender>
        <checklink>
            <mailfrom>nobody@nowhere.com</mailfrom>
            <mailto>nobody@nowhere.com</mailto>
            <mailtemplate>/system/workplace/templates/urlcheckform</mailtemplate>
        </checklink>
        <tempfileproject></tempfileproject>
        <UseWpPicturesFromVFS>false</UseWpPicturesFromVFS>
        <searchbylucene>off</searchbylucene>
        <history>
            <enabled>true</enabled>
            <weeks></weeks>
            <deleteversions>false</deleteversions>
        </history>
    </system>
    <modules/>
</registry>
