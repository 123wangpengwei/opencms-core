var treeWindow = null;
function goLinkSet(theTextbox){
	treeWindow = window.open('folder_tree.html?foldertree=%2F&formname=NAVPROP&variable='+theTextbox+'&viewfile=yes&onlineselect=yes', 'explorer_viewer', 'toolbar=no,location=no,directories=no,status=no,menubar=0,scrollbars=yes,resizable=yes,top=150,left=460,width=150,height=300');
