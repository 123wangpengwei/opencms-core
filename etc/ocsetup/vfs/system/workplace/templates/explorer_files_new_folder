	         treeWindow.opener = self;
	      }
   		}
	treeWindow.focus();
}

function closeAllWindows(){
	if (SelColorWindow != null){
		SelColorWindow.close();
	}
	if (picBrowserWindow != null){
		picBrowserWindow.close();
	}
	if (treeWindow != null){
		treeWindow.close();
	}
}
