function checkval(whichform){
var okay=true;
	 for(i=0;i<whichform.value.length;i++){
	    if(whichform.value.charAt(i)=="%"){
	 	   alert(']]><LABEL value="error.message.badcharacter" /><![CDATA['+whichform.value.charAt(i));
