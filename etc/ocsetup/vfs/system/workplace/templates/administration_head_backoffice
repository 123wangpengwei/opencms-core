<SCRIPT LANGUAGE="JavaScript">
    var pfad="]]><METHOD name="picsUrl"/><![CDATA[";
     function doEdit(para) {
           var backofficepagevalue="";
           if(document.getElementById("backofficepage")){
            backofficepagevalue=document.getElementById("backofficepage").value;
           }
       if(window.top.body.admin_content.doUpdate!=null)window.top.body.admin_content.doUpdate();
     
             switch(para) {
             
             case 1: {
                     parent.frames[1].document.getElementById("Editor").action.value = "exit";
                     if(parent.frames[1].document.getElementById("Editor").backofficepage)parent.frames[1].document.getElementById("Editor").backofficepage.value = backofficepagevalue;
                     parent.frames[1].document.getElementById("Editor").submit();
                     break;
                     }
             case 2: {
                     parent.frames[1].document.getElementById("Editor").action.value = "saveexit";
                     if(parent.frames[1].document.getElementById("Editor").backofficepage)parent.frames[1].document.getElementById("Editor").backofficepage.value = backofficepagevalue;
                     parent.frames[1].document.getElementById("Editor").submit();
                     break;
                     }
             case 3: {
                     parent.frames[1].document.getElementById("Editor").action.value = "save";
                     if(parent.frames[1].document.getElementById("Editor").backofficepage)parent.frames[1].document.getElementById("Editor").backofficepage.value = backofficepagevalue;
                     parent.frames[1].document.getElementById("Editor").submit();
                     break;
                     }
             case 4: {
                     if(parent.frames[1].document.getElementById("Editor").backofficepage)parent.frames[1].document.getElementById("Editor").backofficepage.value = backofficepagevalue;
                     parent.frames[1].document.getElementById("Editor").submit();
                     break;
                     }
               }
           }         
</SCRIPT>
<SCRIPT LANGUAGE="JavaScript">

 function openwinfull(url)
 {
    if (url != '#') {
        w=screen.availWidth-50;
        h=screen.availHeight-200;
        workplace = window.open(url,'preview', 'toolbar=yes,location=yes,directories=no,status=yes,menubar=1,scrollbars=yes,resizable=yes,width='+w+',height='+h);
        if(workplace != null) {
            workplace.focus();
        }

    }
 }
</SCRIPT>

