<SCRIPT LANGUAGE="JavaScript">
    var pfad="]]><METHOD name="picsUrl"/><![CDATA[";
     function doEdit(para) {
        switch(para) {
        case 1: {
                window.top.body.admin_content.Editor.action.value = "exit";
                if(this.backofficepage)window.top.body.admin_content.Editor.backofficepage.value = backofficepage.value;
                window.top.body.admin_content.Editor.submit();
                break;
                }
        case 2: {   
                window.top.body.admin_content.Editor.action.value = "saveexit";
                if(this.backofficepage)window.top.body.admin_content.Editor.backofficepage.value = backofficepage.value;
                window.top.body.admin_content.Editor.submit();
                break;
                }
        case 3: {
                window.top.body.admin_content.Editor.action.value = "save";
                if(this.backofficepage)window.top.body.admin_content.Editor.backofficepage.value = backofficepage.value;
                window.top.body.admin_content.Editor.submit();
                break;
                }
          }
      }       
</SCRIPT>
<SCRIPT LANGUAGE="JavaScript">

 function openwinfull(url)
 {
    if (url != '#') {
        w=screen.availWidth-50;
        h=screen.availHeight-200;
        workplace = window.open(url,'preview', 'toolbar=yes,location=yes,directories=no,status=yes,menubar=1,scrollbars=yes,resizable=yes,width='+w+',height='+h);
        if(workplace != null) {
            workplace.focus();
        }

    }
 }
</SCRIPT>

