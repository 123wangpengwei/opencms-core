function doSubmit() {
	submitted = true;
    document.forms.UPLOAD.WEITER.disabled = true;
    document.forms.UPLOAD.CANCEL_FOLDER.disabled = true;
}
function enableContinue() {
    if (! submitted) {
    	document.forms.UPLOAD.WEITER.disabled = false;
    }
}
