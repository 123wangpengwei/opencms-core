    function addoption(bla){
        if(ns){
            if(document.layers.pull.document.swl.selectbox.length<1+nshowmuch){

                //window.alert(nshowmuch);
                //opts[nshowmuch] = new opt(bla,p,l);

                nshowmuch++;

                for(i=0;i<document.layers.pull.document.swl.selectbox.length;i++){
                    document.layers.pull.document.swl.selectbox.options[document.layers.pull.document.swl.selectbox.length] = null;
                }

                temp = new Option(bla);
                document.layers.pull.document.swl.selectbox.options[document.layers.pull.document.swl.selectbox.length] = temp;
                document.layers.pull.document.swl.selectbox.options[document.layers.pull.document.swl.selectbox.length-1].value = document.layers.pull.document.swl.selectbox.length-1;

                if(nshowmuch == 1)document.layers.pull.document.swl.selectbox.options[0].selected=true;

                history.go(0);
            }
        }

        if(ie){
            //opts[document.swl.selectbox.length] = new opt(bla,p,l);
            temp = new Option(bla);
            document.swl.selectbox.options[document.swl.selectbox.length]= temp;
            document.swl.selectbox.options[document.swl.selectbox.length-1].value=document.swl.selectbox.length-1;
        }

    }



    if(ns){
