function deleteAnItem(theList,itemNo) {
var n=theList.length;
if ((n > 0) && (itemNo > -1)) {
	theList.options[itemNo]=null; 
}
}

function checkVal(item) {
	var nums = "0123456789*";  
	var strLength=item.length;
	if(strLength<1){
		return false;
	}
	for (i=0; i<strLength; i++){
		if(nums.indexOf(item.charAt(i)) == -1){
			alert(']]><LABEL value="error.message.badcharacter" /><![CDATA[' + item.charAt(i));
			return false;			
		}
	}
	return true;
}

function addFolder(theList, item1, item2, item3) {  
	if (item1 != null && item1 != "" && item2 != null && checkVal(item2) && item3 != null && checkVal(item3)) {
		var item = item1+"  Version:"+item2+" - "+item3;
		newOption = new Option(item, item, false, false); 
		theList.options[theList.length] = newOption;
		document.MODEXP.ABHV.value="";
		document.MODEXP.MIN.value="";
		document.MODEXP.MAX.value="*";
	}
}

function addEntry(theList, item){
	newOption = new Option(item, item, false, false);
	theList.options[theList.length] = newOption; 
}

function copySelection() { 
	addFolder(document.MODEXP.new_ressources, document.MODEXP.tempFolder.value);
}

function selectAll(theList) {
	var n=theList.length;
	for (i=0; i<n; i++) {
		theList.options[i].selected=true;
	}
	// umgehe das Problem mit Silverstream
	var allResources="";
	if (n>0) {
		for (i=0; i<n; i++) {
			allResources = allResources + theList.options[i].value + ';';
		} 
	}
	document.MODEXP.alldeps.value=allResources;
}

